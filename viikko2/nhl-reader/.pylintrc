[MASTER]
ignore=CVS

[FORMAT]
max-line-length=100

[MESSAGES CONTROL]
disable=raw-checker-failed,
        bad-inline-option,
        locally-disabled,
        file-ignored,
        suppressed-message,
        useless-suppression,
        deprecated-pragma,
        use-symbolic-message-instead,
        missing-module-docstring,
        missing-function-docstring,
        missing-class-docstring,
        too-few-public-methods

[BASIC]
good-names=i,j,k,ex,Run,_,id

[REPORTS]
output-format=text
reports=no
score=no